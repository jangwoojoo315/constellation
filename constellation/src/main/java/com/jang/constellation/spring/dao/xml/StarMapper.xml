<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="com.jang.constellation.spring.dao.StarMapper">
     
 <select id="selectStar" parameterType="com.jang.constellation.spring.app.StarVo" resultType="com.jang.constellation.spring.app.StarVo">
     select * from star_info where name=#{name}
 </select>
    
</mapper>
    