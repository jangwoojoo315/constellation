<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="com.jang.constellation.signin.dao.SigninMapper">
 <!-- 로그인 -->
<select id="signin" resultType="com.jang.constellation.signin.domain.MemberVO">
 select
     userId, userName, userPass, userPhon, userAddr1, userAddr2, userAddr3, regiDate
 from
     member
 where userId = #{userId}
</select>
 </mapper>